                "border.selected": "#00FFE5bb",
                "border.transparent": "#00000000",
                "border.variant": "#00FFE525",

                // Surfaces
                "background": "#062A4050",
                "elevated_surface.background": "#002929FF",
                "surface.background": "#242631ee",

                // Elements
                "element.active": "#796595FF",
                "element.background": "#001A1AFF",
                "element.disabled": "#e9dbfdFF",
                "element.hover": "#002929FF",
                "element.selected": "#002929FF",

                // Ghost elements
                "drop_target.background": "#002929FF",
                "ghost_element.active": "#00FFE550",
                "ghost_element.background": "#062A4000",
                "ghost_element.disabled": "#FFD700FF",
                "ghost_element.hover": "#00FFE535",
                "ghost_element.selected": "#00FFE525",

